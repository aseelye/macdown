source 'https://rubygems.org'

# CocoaPods depends on xcodeproj, which currently requires CFPropertyList < 4.0.
# CFPropertyList 3.0.9 is incompatible with Ruby >= 3.2 (Bundler 4 requires 3.2+),
# so pin to the newest compatible version until xcodeproj relaxes its constraint.
gem 'CFPropertyList', '3.0.8'
gem 'cocoapods', '1.16.2'
